<?xml version="1.0"?>
<PropertyList>

	<!--Plane-->
    <path>orbiter.ac</path>
    <offsets>
        <z-m>0.0</z-m>
        <y-m>0.0</y-m>
        <x-m>0.0</x-m>
        <pitch-deg>0</pitch-deg>
    </offsets>
	<!--Ailerons-->
    <animation>
        <type>rotate</type>
        <object-name>leftAileron</object-name>
        <property>/surface-positions/left-aileron-pos-deg</property>
		<axis>
	        <x1-m>1.076</x1-m>
            <y1-m>-0.920</y1-m>
            <z1-m>-0.021</z1-m>
            <x2-m>0.850</x2-m>
            <y2-m>-0.531</y2-m>
            <z2-m>-0.009</z2-m>
        </axis>
	</animation>

    <animation>
        <type>rotate</type>
        <object-name>rightAileron</object-name>
        <property>/surface-positions/right-aileron-pos-deg</property>
		<axis>
            <x1-m>1.076</x1-m>
            <y1-m>0.920</y1-m>
            <z1-m>-0.021</z1-m>
            <x2-m>0.850</x2-m>
            <y2-m>0.531</y2-m>
            <z2-m>-0.009</z2-m>
        </axis>
    </animation>     
	<!--Propeller-->
    <animation>
        <type>noshadow</type>
        <object-name>prop</object-name>
    </animation>
    <animation>
        <type>spin</type>
        <object-name>prop</object-name>
        <property>/engines/engine[0]/rpm</property>
        <axis>
            <x1-m>0.849</x1-m>
            <y1-m>0.00</y1-m>
            <z1-m>0.00</z1-m>
            <x2-m>0.849</x2-m>
            <y2-m>0.00</y2-m>
            <z2-m>0.00</z2-m>
        </axis>
    </animation>
    <animation>
        <type>select</type>
        <object-name>prop</object-name>
        <condition>
            <less-than>
                <property>/engines/engine[0]/rpm</property>
                <value>1100</value>
            </less-than>
        </condition>
    </animation>
</PropertyList>
